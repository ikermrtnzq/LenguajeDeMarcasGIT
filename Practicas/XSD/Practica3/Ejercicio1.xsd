<!--Declaracion de XSD para clave usando xs:pattern y xs:length:-->
<xs:element name="clave">
  <xs:simpleType>
    <xs:restriction base="xs:string">
      <xs:length value="10"/>
      <xs:pattern value="[a-zA-Z0-9]{10}"/>
    </xs:restriction>
  </xs:simpleType>
</xs:element>


<!--Declaracion de XSD para clave usando Ãºnicamente xs:pattern:-->
<xs:element name="clave">
  <xs:simpleType>
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-zA-Z0-9]{10}"/>
    </xs:restriction>
  </xs:simpleType>
</xs:element>
