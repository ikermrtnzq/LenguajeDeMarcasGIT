<!--Declaracion de una DTD interna-->
<!DOCTYPE cuadros [
    <!ELEMENT cuadros (cuadro*)>
    <!ELEMENT cuadro EMPTY>
    <!ATTLIST cuadro titulo ID #REQUIRED>
    <!ATTLIST cuadro autor CDATA #REQUIRED>
]>

<!--Declaracion de cuadros-->
<cuadros>
    <!--Declaracion de un cuadro-->
    <cuadro titulo="Adán y Eva" autor="Lucas Cranach, el viejo"/>

    <!--Declaracion de un cuadro-->
    <cuadro titulo="El Solitario Viajero" autor="Ana María López García"/>
</cuadros>