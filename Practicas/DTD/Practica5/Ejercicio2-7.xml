<!--Declaracion de una DTD interna-->
<!DOCTYPE colores [
    <!ELEMENT colores (color+)>
    <!ELEMENT nombreSvg (#PCDATA)>
    <!ELEMENT codigo (CMYK | rgb)>
    <!ELEMENT rgb (#PCDATA)>
    <!ELEMENT cmyk (#PCDATA)>
]>

<!--Declaracion de colores -->
<colores>
    <!--Declaracion de un color -->
    <color>
        <nombreSvg>Purple</nombreSvg>
        
        <!--Declaracion de un codigo -->
        <codigo>
            <rgb>#800080</rgb>
        </codigo>
    </color>
    
<!--Declaracion de un color -->
    <color>
        <nombreSvg>Purple</nombreSvg>

        <!--Declaracion de un codigo -->
        <codigo>
            <cmyk>#00FF007F</cmyk>
        </codigo>
    </color>
</colores>