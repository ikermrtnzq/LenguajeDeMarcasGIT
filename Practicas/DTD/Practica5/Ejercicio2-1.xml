<!--Definicion de una DTD interna-->
<!DOCTYPE favoritos [
  <!ELEMENT marcadores (marcador)>
  <!ELEMENT marcador (nombre, uri)>
  <!ELEMENT nombre (#PCDATA)>
  <!ELEMENT uri (#PCDATA)>
]>

<!--Definicion de marcadores-->
<marcadores>
    <!--Definicion de un marcador-->
    <marcador>
        <nombre>W3C</nombre>
        <uri>http://www.w3.org/</uri>
    </marcador>

    <!--Definicion de un marcador-->
    <marcador>
        <nombre>Web Hypertext Application Technology Working Group (WHATWG)</nombre>
        <uri>http://www.whatwg.org/</uri>
    </marcador>
</marcadores>